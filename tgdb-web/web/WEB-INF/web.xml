        <servlet-name>PanelRightTop</servlet-name>
        <servlet-class>org.tgdb.frame.PanelRightTop</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Foot</servlet-name>
        <servlet-class>org.tgdb.frame.Foot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>olsajax</servlet-name>
        <servlet-class>org.tgdb.servlets.olsajax</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletRedirector</servlet-name>
        <url-pattern>/ServletRedirector</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletTestRunner</servlet-name>
        <url-pattern>/ServletTestRunner</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NavMenu</servlet-name>
        <url-pattern>/NavMenu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileServlet</servlet-name>
