    <servlet>
        <servlet-name>NavMenu</servlet-name>
        <servlet-class>org.tgdb.webapp.NavMenu</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileServlet</servlet-name>
        <servlet-class>org.tgdb.webapp.FileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controller</servlet-name>
        <servlet-class>org.tgdb.frame.Controller</servlet-class>
