        <servlet-name>PanelLeft</servlet-name>
        <url-pattern>/PanelLeft</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Header</servlet-name>
        <url-pattern>/Header</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PanelRightTop</servlet-name>
        <url-pattern>/PanelRightTop</url-pattern>
    </servlet-mapping>
