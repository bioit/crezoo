    <servlet-mapping>
        <servlet-name>olsajax</servlet-name>
        <url-pattern>/olsajax</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>
            Controller
        </welcome-file>
    </welcome-file-list>
