    <servlet>
        <servlet-name>Menu</servlet-name>
        <servlet-class>org.tgdb.frame.MenuAdvanced</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Bottom</servlet-name>
        <servlet-class>org.tgdb.frame.Bottom</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImageServlet</servlet-name>
        <servlet-class>org.tgdb.servlets.ImageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WSServlet_tgdbws</servlet-name>
        <servlet-class>org.tgdb.ws.tgdbwsImpl</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>service</servlet-name>
        <servlet-class>org.tgdb.ws.service</servlet-class>
