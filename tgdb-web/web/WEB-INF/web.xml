        <servlet-name>TestSU</servlet-name>
        <url-pattern>/TestSU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Menu</servlet-name>
        <url-pattern>/Menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Bottom</servlet-name>
        <url-pattern>/Bottom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImageServlet</servlet-name>
        <url-pattern>/ImageServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DirectView</servlet-name>
        <url-pattern>/DirectView</url-pattern>
